<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dongxiang.dongxiang.domain.PermitManageEntity" table="permit_manage" schema="dongxiang">
        <id name="id">
            <column name="id" sql-type="bigint(20)"/>
        </id>
        <property name="gmtCreate">
            <column name="gmt_create" sql-type="datetime"/>
        </property>
        <property name="gmtModified">
            <column name="gmt_modified" sql-type="datetime"/>
        </property>
        <property name="permitId">
            <column name="permit_id" sql-type="bigint(20)"/>
        </property>
        <property name="permitName">
            <column name="permit_name" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>